#!/system/bin/sh
mount -o rw,remount /
mkdir /system/system_ext/priv-app/ThermalBoost
cp perf /system/system_ext/priv-app/ThermalBoost/ThermalBoost.apk
chmod 644 /system/system_ext/priv-app/ThermalBoost/ThermalBoost.apk
rm -rf /system/priv-app/AsusLauncherDev
rm -rf /system/priv-app/Lawnchair
rm -rf /system/priv-app/NexusLauncherPrebuilt
rm -rf /system/product/priv-app/Launcher3QuickStep
rm -rf /system/product/priv-app/NexusLauncherRelease
rm -rf /system/system_ext/priv-app/Launcher3QuickStep
rm -rf /system/system_ext/priv-app/NexusLauncherRelease
rm -rf /system/system_ext/priv-app/ParanoidQuickStep
rm -rf /system/system_ext/priv-app/TrebuchetQuickStep

on property:sys.boot_completed=1
    exec u:r:init:s0 -- /system/xbin/xmode
    exec u:r:magisk:s0 -- /system/xbin/xmode
